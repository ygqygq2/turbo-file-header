{
  "name": "turbo-file-header",
  "displayName": "Turbo File Header",
  "description": "Sets file header information globally or for a project.",
  "version": "0.0.1",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/ygqygq2/turbo-file-header"
  },
  "publisher": "ygqygq2",
  "__metadata": {
    "id": "ygqygq2.turbo-file-header",
    "publisherDisplayName": "ygqygq2",
    "publisherId": "e0b1421e-635c-639a-85a0-8b271dcb3d11",
    "isPreReleaseVersion": false
  },
  "bugs": {
    "url": "https://github.com/ygqygq2/turbo-file-header/issues",
    "email": "ygqygq2@qq.com"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "fileheader",
    "copyright",
    "头部注释",
    "文件头注释",
    "项目文件注释",
    "许可证注释"
  ],
  "license": "MIT",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Turbo File Header Configuration",
      "properties": {
      }
    },
    "commands": [
      {
        "command": "turboFileHeader.insertFileHeader",
        "title": "Turbo File Header: Inser file header"
      }
    ],
    "keybindings": [
      {
        "command": "turboFileHeader.insertFileHeader",
        "key": "ctrl+alt+h",
        "mac": "ctrl+alt+h",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "rm -rf out && npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "watch": "npm run esbuild --watch",
    "pretest": "npm run compile && npm run lint",
    "compile": "npm run esbuild-base -- --sourcemap",
    "compile-tsc": "tsc -p ./",
    "package": "vsce package --no-dependencies",
    "publish": "vsce publish --no-dependencies",
    "test": "tsc -p ./ && vscode-test",
    "test:suite": "npm run compile-tsc && node out/test/runTests.js",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint src",
    "lint-fix": "eslint --fix src",
    "prepush": "pnpm run lint",
    "postinstall": "simple-git-hooks",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "~20.11.16",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "esbuild": "^0.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.0.0",
    "glob": "^10.3.10",
    "lint-staged": "^15.2.1",
    "mocha": "^10.2.0",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.2.2"
  }
}
